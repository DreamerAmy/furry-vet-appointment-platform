{
  "name": "furry-vet-appointment-platform",
  "version": "1.0.0",
  "description": "A Full Stack Vet Appointment Booking Application using MongoDB, Express, React, Node, Javascript",
  "main": "index.js",
  "engines": {
    "node": "15.7.0",
    "npm": "7.4.3"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreamerAmy/furry-vet-appointment-platform.git"
  },
  "author": "",
