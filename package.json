  "bugs": {
    "url": "https://github.com/DreamerAmy/furry-vet-appointment-platform/issues"
  },
  "homepage": "https://github.com/DreamerAmy/furry-vet-appointment-platform#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.6.5",
    "nodemon": "^2.0.20",
    "popper.js": "^1.16.1",
    "react-scripts": "^5.0.1"
  }
